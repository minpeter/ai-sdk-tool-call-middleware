{
  "name": "ai-sdk-tool",
  "private": true,
  "scripts": {
    "build": "turbo run build --parallel",
    "test": "turbo run test --parallel",
    "test:watch": "turbo run test:watch --parallel",
    "dev": "turbo run dev",
    "lint": "turbo run lint --parallel",
    "lint:fix": "turbo run lint:fix --parallel && prettier --write \"{packages,examples}/**/*.{ts,tsx,js,mjs,cjs,json,md,yaml,yml}\" \"docs/**/*.md\" \"*.{md,json}\"",
    "fmt": "prettier --check \"{packages,examples}/**/*.{ts,tsx,js,mjs,cjs,json,md,yaml,yml}\" \"docs/**/*.md\" \"*.{md,json}\"",
    "fmt:fix": "pnpm run lint:fix",
    "check-types": "turbo run check-types --parallel",
    "changeset": "changeset",
    "ci:release": "turbo build --parallel && changeset publish",
    "ci:version": "changeset version",
    "prepare": "husky"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.6",
    "@eslint/js": "^9.34.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "eslint": "^9.34.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "turbo": "^2.5.6",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.41.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@9.14.4",
  "engines": {
    "node": ">=18"
  },
  "license": "Apache-2.0"
}
