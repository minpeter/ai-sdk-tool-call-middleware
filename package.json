{
  "name": "ai-sdk-tool",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "fmt": "prettier --check \"**/*.{ts,tsx,md}\"",
    "fmt:fix": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "changeset": "changeset",
    "ci:release": "turbo build && changeset publish",
    "ci:version": "changeset version",
    "prepare": "husky"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.6",
    "@eslint/js": "^9.17.0",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.0",
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.17.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@9.14.4",
  "engines": {
    "node": ">=18"
  }
}