{
  "name": "ai-sdk-tool",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "changeset": "changeset",
    "ci:release": "turbo build && changeset publish",
    "ci:version": "changeset version"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.5",
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@9.14.4",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "overrides": {
      "zod": "^4.0.17"
    }
  }
}