{
  "name": "@ai-sdk-tool/parser",
  "version": "4.0.1",
  "description": "AI SDK middleware for tool call parsing",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/minpeter/ai-sdk-tool-call-middleware"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./community": {
      "types": "./dist/community.d.ts",
      "import": "./dist/community.js",
      "require": "./dist/community.cjs"
    },
    "./rxml": {
      "types": "./dist/rxml.d.ts",
      "import": "./dist/rxml.js",
      "require": "./dist/rxml.cjs"
    },
    "./rjson": {
      "types": "./dist/rjson.d.ts",
      "import": "./dist/rjson.js",
      "require": "./dist/rjson.cjs"
    },
    "./schema-coerce": {
      "types": "./dist/schema-coerce.d.ts",
      "import": "./dist/schema-coerce.js",
      "require": "./dist/schema-coerce.cjs"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm clean && tsup --tsconfig tsconfig.build.json",
    "build:watch": "pnpm clean && tsup --watch --tsconfig tsconfig.build.json",
    "clean": "rimraf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "check:biome": "biome check",
    "check:types": "tsc --noEmit",
    "check": "run-p check:*",
    "fmt:biome": "biome check --write",
    "fmt": "run-p fmt:*",
    "changeset": "changeset",
    "ci:release": "pnpm build && changeset publish",
    "ci:version": "changeset version"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.29",
    "@ai-sdk/provider": "3.0.8",
    "@ai-sdk/provider-utils": "4.0.15",
    "dedent": "^1.7.1",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@ai-sdk/openai-compatible": "2.0.30",
    "@biomejs/biome": "2.4.0",
    "@changesets/cli": "2.29.8",
    "@types/node": "^25.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "ai": "6.0.86",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "5.9.3",
    "ultracite": "7.2.1",
    "vitest": "^4.0.18",
    "zod": "^4.3.6"
  },
  "packageManager": "pnpm@9.14.4",
  "engines": {
    "node": ">=18"
  },
  "license": "Apache-2.0"
}
