{
  "name": "@ai-sdk-tool/eval",
  "version": "1.1.0",
  "description": "Benchmarking and evaluation tools for AI SDK",
  "license": "Apache-2.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/minpeter/ai-sdk-tool-call-middleware",
    "directory": "packages/eval"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "data"
  ],
  "scripts": {
    "build": "pnpm clean && tsup --tsconfig tsconfig.build.json",
    "build:watch": "pnpm clean && tsup --watch --tsconfig tsconfig.build.json",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "test:e2e": "tsup src/run-test.ts --format esm --clean --out-dir dist --no-config && node dist/run-test.js"
  },
  "dependencies": {
    "@ai-sdk/provider": "3.0.1",
    "@ai-sdk-tool/middleware": "workspace:*",
    "ai": "6.0.6",
    "ajv": "^8.17.1",
    "decimal.js": "^10.4.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@ai-sdkx/tsconfig": "workspace:*",
    "@types/node": "^25.0.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
