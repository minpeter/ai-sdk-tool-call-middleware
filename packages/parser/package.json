{
  "name": "@ai-sdk-tool/parser",
  "version": "3.1.3",
  "description": "AI SDK middleware for tool call parsing",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/minpeter/ai-sdk-tool-call-middleware",
    "directory": "packages/parser"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./community": {
      "types": "./dist/community.d.ts",
      "import": "./dist/community.js",
      "require": "./dist/community.cjs"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm clean && tsup --tsconfig tsconfig.build.json",
    "build:watch": "pnpm clean && tsup --watch --tsconfig tsconfig.build.json",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@ai-sdk-tool/schema-coerce": "workspace:*",
    "@ai-sdk-tool/rjson": "workspace:*",
    "@ai-sdk-tool/rxml": "workspace:*",
    "@ai-sdk/provider": "3.0.4",
    "@ai-sdk/provider-utils": "4.0.8",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@ai-sdk-tool/tsconfig": "workspace:*",
    "@ai-sdk/openai-compatible": "2.0.13",
    "@types/node": "^25.0.9",
    "ai": "6.0.38",
    "tsup": "^8.5.1",
    "zod": "^4.3.5"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0"
}
