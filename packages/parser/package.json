{
  "name": "@ai-sdk-tool/parser",
  "version": "3.0.0-canary.0",
  "description": "",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./community": {
      "types": "./dist/community.d.ts",
      "import": "./dist/community.js",
      "require": "./dist/community.cjs"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm clean && tsup --tsconfig tsconfig.build.json",
    "build:watch": "pnpm clean && tsup --watch --tsconfig tsconfig.build.json",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@ai-sdk-tool/rxml": "workspace:*",
    "@ai-sdk/provider": "3.0.0-beta.14",
    "@ai-sdk/provider-utils": "4.0.0-beta.30"
  },
  "devDependencies": {
    "@ai-sdkx/tsconfig": "workspace:*",
    "@ai-sdk/openai-compatible": "2.0.0-beta.31",
    "@types/node": "^24.10.0",
    "ai": "6.0.0-beta.92",
    "tsup": "^8.5.0",
    "zod": "^4.1.12"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0"
}
