{
  "name": "@ai-sdk-tool/parser",
  "version": "3.0.0",
  "description": "AI SDK middleware for tool call parsing",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/minpeter/ai-sdk-tool-call-middleware",
    "directory": "packages/parser"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./v5": {
      "types": "./dist/v5.d.ts",
      "import": "./dist/v5.js",
      "require": "./dist/v5.cjs"
    },
    "./v6": {
      "types": "./dist/v6.d.ts",
      "import": "./dist/v6.js",
      "require": "./dist/v6.cjs"
    },
    "./community": {
      "types": "./dist/community.d.ts",
      "import": "./dist/community.js",
      "require": "./dist/community.cjs"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm clean && tsup --tsconfig tsconfig.build.json",
    "build:watch": "pnpm clean && tsup --watch --tsconfig tsconfig.build.json",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@ai-sdk-tool/rxml": "workspace:*",
    "@ai-sdk/provider": "3.0.1",
    "@ai-sdk/provider-utils": "4.0.2",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@ai-sdk/openai-compatible": "2.0.2",
    "@ai-sdk/provider-v5": "npm:@ai-sdk/provider@2.0.1",
    "@ai-sdk/provider-v6": "npm:@ai-sdk/provider@3.0.1",
    "@ai-sdkx/tsconfig": "workspace:*",
    "@types/node": "^25.0.3",
    "ai": "6.0.5",
    "ai-v5": "npm:ai@5.0.117",
    "ai-v6": "npm:ai@6.0.5",
    "tsup": "^8.5.1",
    "zod": "^4.3.4"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0"
}
