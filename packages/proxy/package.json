{
  "name": "@ai-sdk-tool/proxy",
  "version": "0.1.0",
  "description": "OpenAI-compatible proxy server for AI SDK tool middleware",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/minpeter/ai-sdk-tool-call-middleware",
    "directory": "packages/proxy"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm clean && tsup --tsconfig tsconfig.build.json",
    "build:watch": "pnpm clean && tsup --watch --tsconfig tsconfig.build.json",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@ai-sdk/provider": "3.0.3",
    "@ai-sdk/provider-utils": "4.0.7",
    "@fastify/cors": "^11.2.0",
    "ai": "6.0.37",
    "fastify": "^5.7.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@ai-sdk-tool/parser": "workspace:*",
    "@ai-sdk/openai-compatible": "2.0.12",
    "@ai-sdkx/tsconfig": "workspace:*",
    "@types/node": "^25.0.9",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "tsup": "^8.5.1",
    "vitest": "^4.0.17"
  },
  "keywords": [
    "ai",
    "sdk",
    "proxy",
    "openai",
    "tools",
    "middleware"
  ],
  "author": "",
  "license": "Apache-2.0"
}
