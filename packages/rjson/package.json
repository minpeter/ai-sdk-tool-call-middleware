{
  "name": "@ai-sdk-tool/rjson",
  "version": "0.1.0",
  "description": "Relaxed JSON parser with tolerant mode, duplicate key handling, and JSON5-like syntax support",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/minpeter/ai-sdk-tool-call-middleware",
    "directory": "packages/rjson"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm clean && tsup --tsconfig tsconfig.build.json",
    "build:watch": "pnpm clean && tsup --watch --tsconfig tsconfig.build.json",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "devDependencies": {
    "@ai-sdk-tool/tsconfig": "workspace:*",
    "@types/node": "^25.0.9",
    "tsup": "^8.5.1"
  },
  "keywords": [
    "json",
    "parser",
    "relaxed",
    "tolerant",
    "json5"
  ],
  "author": "",
  "license": "Apache-2.0"
}
