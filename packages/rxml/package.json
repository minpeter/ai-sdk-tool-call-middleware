{
  "name": "@ai-sdk-tool/rxml",
  "version": "0.1.2",
  "description": "Robust XML parser/streamer/builder for AI-generated XML",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/minpeter/ai-sdk-tool-call-middleware",
    "directory": "packages/rxml"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm clean && tsup --tsconfig tsconfig.build.json",
    "build:watch": "pnpm clean && tsup --watch --tsconfig tsconfig.build.json",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "devDependencies": {
    "@ai-sdkx/tsconfig": "workspace:*",
    "@types/node": "^25.0.3",
    "tsup": "^8.5.1",
    "zod": "^4.3.5"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0"
}
